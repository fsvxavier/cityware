{
    "name": "fsvxavier/cityware",
    "description": "ZF2 Cityware Library",
    "type": "library",
    "version": "1.2.0",
	"license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "zf2",
        "library"
    ],
	"require-dev": {
        "phpunit/PHPUnit": "~4.6",
        "phpunit/phpcov": "~2.0"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.4.*"		
    },
	"suggest": {
        "zendframework/zendpdf": "ZendPdf for creating PDF representations of barcodes",
		"twitter/bootstrap": "3.3.*@dev",
		"neilime/zf2-twb-bundle": "1.0.*@dev"
    },
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fsvxavier/cityware.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Cityware\\": "library/Cityware/"
        }
    }
}