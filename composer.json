{
    "name": "fsvxavier/cityware",
    "description": "ZF2 Cityware Library",
    "type": "library",
    "version": "1.2.0",
	"license": "GPL-3.0+",
    "keywords": [
        "framework",
        "zf2",
        "library"
    ],
	"authors": [
        {
            "name": "Fabricio S V Xavier",
            "email": "fabricio@cityware.com.br",
            "homepage": "http://www.cityware.com.br",
            "role": "Developer"
        }
    ],
	"require-dev": {
        "phpunit/PHPUnit": "~4.6",
        "phpunit/phpcov": "~2.0"
    },
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "~2.3"
    },
	"suggest": {
        "zendframework/zendpdf": "ZendPdf for creating PDF representations of barcodes",
		"twitter/bootstrap": "3.3.*@dev",
		"neilime/zf2-twb-bundle": "1.0.*@dev"
    },
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fsvxavier/cityware.git"
        }
    ],
    "autoload": {
        "psr-0": {
            "Cityware\\": "library/"
        }
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}